  <name>andAgain</name>
  <description />
  <settings>
    <build-runners>
      <runner id="simpleRunner" name="" type="simpleRunner">
        <parameters>
          <param name="script.content" value="echo a &gt; 1.txt" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <build-extensions>
      <extension id="matrix" type="matrix">
        <parameters>
          <param name="groupArtifactsByBuild" value="true" />
          <param name="matrix.param.a.1" value="a" />
          <param name="matrix.value.a.1" value="1" />
          <param name="matrix.value.a.2" value="3" />
        </parameters>
      </extension>
    </build-extensions>
  </settings>
</build-type>

